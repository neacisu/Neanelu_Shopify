{
  "name": "@app/database",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "db:seed:taxonomy": "node --import tsx src/seed/taxonomy.ts",
    "db:seed": "node --import tsx src/seed/products.ts",
    "test": "node --test --experimental-test-isolation=none src/**/*.test.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "drizzle-orm": "^0.45.1",
    "pg": "^8.16.3",
    "pg-copy-streams": "^7.0.0",
    "stream-json": "^1.9.1"
  },
  "devDependencies": {
    "@types/pg": "^8.16.0",
    "@types/pg-copy-streams": "^1.2.5",
    "drizzle-kit": "^0.31.8"
  }
}
